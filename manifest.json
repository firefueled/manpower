{

  "manifest_version": 2,
  "name": "ManPower",
  "version": "0.1",
 
  "description": "View man pages",
  "icons": {
    "48": "icons/manpower-48.png",
    "96": "icons/manpower-96.png"
  },

  "applications": {
    "gecko": {
      "id": "manpower@firefueld.com",
      "strict_min_version": "45.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["manpower.js"]
    }
  ]

}